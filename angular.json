{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ats": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "sass",
          "flat": true
        }
      },
      "root": "projects/ats",
      "sourceRoot": "projects/ats/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ats",
            "index": "projects/ats/src/index.html",
            "main": "projects/ats/src/main.ts",
            "polyfills": "projects/ats/src/polyfills.ts",
            "tsConfig": "projects/ats/tsconfig.app.json",
            "aot": false,
            "assets": [
              "projects/ats/src/favicon.ico",
              "projects/ats/src/assets"
            ],
            "styles": [
              "projects/ats/src/styles.sass"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [{
                "replace": "projects/ats/src/environments/environment.ts",
                "with": "projects/ats/src/environments/environment.prod.ts"
              }],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [{
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ats:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ats:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ats:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ats/src/test.ts",
            "polyfills": "projects/ats/src/polyfills.ts",
            "tsConfig": "projects/ats/tsconfig.spec.json",
            "karmaConfig": "projects/ats/karma.conf.js",
            "assets": [
              "projects/ats/src/favicon.ico",
              "projects/ats/src/assets"
            ],
            "styles": [
              "projects/ats/src/styles.sass"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ats/tsconfig.app.json",
              "projects/ats/tsconfig.spec.json",
              "projects/ats/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/ats/e2e/protractor.conf.js",
            "devServerTarget": "ats:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ats:serve:production"
            }
          }
        }
      }
    },
    "domain": {
      "projectType": "library",
      "root": "projects/domain",
      "sourceRoot": "projects/domain/src",
      "prefix": "domain",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/domain/tsconfig.lib.json",
            "project": "projects/domain/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/domain/src/test.ts",
            "tsConfig": "projects/domain/tsconfig.spec.json",
            "karmaConfig": "projects/domain/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/domain/tsconfig.lib.json",
              "projects/domain/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "ats"
}
